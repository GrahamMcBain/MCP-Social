{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(tools|auth)/(.*)",
      "dest": "/src/server.ts"
    },
    {
      "src": "/.*",
      "dest": "/src/server.ts"
    }
  ]
}
